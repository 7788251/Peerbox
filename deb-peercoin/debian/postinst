#!/bin/sh
# postinst script for ppcoin
#
# see: dh_installdeb(1)

set -e

case "$1" in
    configure)
	
	systemctl daemon-reload
	echo ""
	echo "Welcome to Peerbox."
	echo ""

    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0