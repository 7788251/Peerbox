pkgname=mintbox
pkgver=0.1
pkgrel=2
pkgdesc="Small wrapper around ppcoind walletpassphrase"
provides=('mintbox')
makedepends=('python2')
depends=('python2-sh' 'python2-colored')
arch=('any')
license=('GPL')
source=( "${pkgname}.py" )

sha256sums=('f14aa4e3839372aebafa37c335f1bddcf9efd33e3375cde5671b53d770778376')

package() {
install -Dm755 ${pkgname}.py ${pkgdir}/usr/bin/${pkgname}
}
